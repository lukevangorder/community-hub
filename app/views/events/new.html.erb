<%= form_for @event do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :info %><br>
    <%= f.text_area :info %><br>
    <%= f.label :starts_at %><br>
    <%= f.date_select :starts_at%><br>
    <%= f.label :ends_at %><br>
    <%= f.date_select :ends_at%><br>
    <%= f.submit %>
<% end %>